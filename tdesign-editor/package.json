{
  "name": "tdesign-editor",
  "version": "1.0.0",
  "description": "",
  "title": "tdesign-editor",
  "main": "lib/app.js",
  "module": "lib/app.js",
  "typings": "lib/app.d.ts",
  "scripts": {
    "prebuild": "rimraf es/* lib/* dist/* esm/*",
    "build": "rollup -c script/rollup.config.mjs && npm run build:tsc",
    "dev": "rollup -c script/rollup.config.mjs -w",
    "dev-w": "concurrently \"rollup -c script/rollup.config.mjs -w\" \"tsc --emitDeclarationOnly -d -p ./tsconfig.dev.json --outDir lib/ -w\"",
    "build:tsc": "tsc --emitDeclarationOnly -d -p ./tsconfig.build.json --outDir lib/"
  },
  "files": [
    "esm",
    "es",
    "lib",
    "cjs",
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "tdesign editor",
    "fabric",
    "canvas",
    "photo editor",
    "design editor"
  ],
  "author": "冰河末日",
  "license": "ISC",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.6.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@types/react": "^18.3.3",
    "concurrently": "^8.2.2",
    "eslint": "8.x",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.8.0",
    "rimraf": "^6.0.0",
    "rollup": "^4.18.1",
    "rollup-plugin-multi-input": "^1.4.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.16.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.7",
    "mobx": "^6.13.0",
    "mobx-react-lite": "^4.0.7",
    "mobx-state-tree": "^6.0.1",
    "nanoid": "^5.0.7",
    "styled-components": "^6.1.11"
  }
}
